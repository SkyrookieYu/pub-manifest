{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "ItemList.schema.json",
    "title": "schema.org ItemList object",
    "type": "object",
    "properties": {
        "type": {
            "anyOf": [
                {
                    "type": "string",
                    "const": "ItemList"
                },
                {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "contains": {
                        "const": "ItemList"
                    }
                }
            ]
        },
        "itemListElement": {
            "type": [
            	"array"
            ],
            "items": {
                "type": "string"
            },
            "uniqueItems": true
        }
    },
    "required": [
        "type",
        "itemListElement" 
    ]
}
